    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Scan",
            "type": "debugpy",
            "request": "launch",
            "module": "archivuelo",
            "args": [
                "--verbose",
                "scan",
                // "--clear-db",
            ]
        },
        {
            "name": "Python Debugger: Import",
            "type": "debugpy",
            "request": "launch",
            "module": "archivuelo",
            "args": [
                "--verbose",
                "import",
                "C:\\Users\\sebre\\Development\\_temp\\import_media_tmp_2",
                "--use-cache",
                "--exclude-before",
                "2024-10-15",
                "--exclude-after",
                "2024-10-16",
                "--overwrite",
                "--force-all",
            ]
        }
    ]
}